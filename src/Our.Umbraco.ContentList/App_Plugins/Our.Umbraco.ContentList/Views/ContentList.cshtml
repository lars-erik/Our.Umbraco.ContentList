@using Newtonsoft.Json
@using Our.Umbraco.ContentList.Models
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<Newtonsoft.Json.Linq.JObject>
@{
    var configuration = JsonConvert.DeserializeObject<ContentListConfiguration>(Model["config"].ToString());
}

@await Component.InvokeAsync("ContentList", new {configuration, contextContent=Umbraco.AssignedContentItem})
